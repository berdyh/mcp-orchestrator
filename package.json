{
  "name": "mcp-meta-orchestrator",
  "version": "0.1.0",
  "description": "MCP Meta-Orchestrator Server - Dynamic MCP discovery and orchestration system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "generate-encryption-key": "node scripts/generate-encryption-key.js",
    "docker:build": "docker build -t mcp-meta-orchestrator .",
    "docker:dev": "docker-compose up --build",
    "docker:test": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "orchestrator",
    "automation",
    "development-tools"
  ],
  "author": "MCP Meta-Orchestrator Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "tslib": "^2.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
