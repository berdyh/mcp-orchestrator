{
  "version": 2,
  "builds": [
    {
      "src": "dist/servers/http-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "dist/servers/http-server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "dist/servers/http-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/servers/http-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "error"
  }
}
